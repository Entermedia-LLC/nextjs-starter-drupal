{
    "name": "entermedia-llc/nextjs-starter-d9",
    "description": "A base profile for Drupal 9.",
    "type": "drupal-custom-profile",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Entermedia\\NextjsStarterD9\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    },
    "require": {
        "drupal/next": "^1.6",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_login": "^2.0",
        "drupal/anonymous_redirect": "^2.2@RC",
        "drupal/admin_toolbar": "^3.3",
        "drupal/devel": "^5.1",
        "drupal/devel_kint_extras": "^1.1",
        "drupal/config_split": "^1.9",
        "drupal/config_ignore": "^2.3",
        "drupal/jsonapi_extras": "^3.23",
        "drupal/jsonapi_menu_items": "^1.2",
        "drupal/jsonapi_views": "^1.0",
        "drupal/module_filter": "^4.0",
        "drupal/metatag": "^1.22",
        "drupal/redirect": "^1.8",
        "drupal/structure_sync": "^2.0",
        "drupal/field_group": "^3.4"
    }
}
